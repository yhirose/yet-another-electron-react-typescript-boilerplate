{
  "name": "app",
  "version": "0.1.0",
  "description": "React-TypeScript-Electron boilerplate with Create React App and Electron Builder",
  "author": "Your Name",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently \"BROWSER=none yarn start\" \"tsc -p ./electron -w\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:pack": "yarn build && tsc -p ./electron && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.your.app",
    "mac": {
      "category": "your.app.category.type",
      "icon": "electron/assets/electron.icns"
    },
    "productName": "DemoApp",
    "copyright": "Copyright Â© 2019 Your Name",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.7.0",
    "wait-on": "^5.0.1"
  }
}
